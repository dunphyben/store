<h1>Log In</h1>

<%= form_tag sessions_path do |f| %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag("Log In", :class => "btn btn-primary") %>
<% end %>
